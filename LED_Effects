class LedMode(enum){
       STEDY = 0x28
       BREATHE = 0x22

       BLINK_RESPIRATION = 0x42
       BLINK_NEON = 0x44
       BLINK_STEDY = 0x48

       STEDY_RESPIRATION = 0x82
       STEDY_NEON = 0x84
       STEDY_STEDY = 0x88

       S =  0b00101000
       B =  0b00100010
       BR = 0b01000010
       BN = 0b01000100
       BS = 0b01001000 

       SR = 0b10000010
       SN = 0b10000100 
       SS = 0b10001000
}
